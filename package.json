{
  "name": "trim-margin",
  "version": "1.0.1",
  "description": "\"trimMargin\" like Kotlin and \"stripMargin\" like Scala.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "private": false,
  "scripts": {
    "prepublish": "npm run lib",
    "lib": "tsc -p src/ts/tsconfig.json --outDir ./lib",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tee-talog/trim-margin.git"
  },
  "keywords": [
    "JavaScript",
    "npm module",
    "trimMargin",
    "stripMargin"
  ],
  "author": "tee-talog",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tee-talog/trim-margin/issues"
  },
  "homepage": "https://github.com/tee-talog/trim-margin#readme",
  "devDependencies": {
    "@types/jest": "^22.1.0",
    "@types/node": "^9.3.0",
    "jest": "^22.0.4",
    "tslib": "^1.9.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "<rootDir>/test-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|js)"
    ]
  }
}
